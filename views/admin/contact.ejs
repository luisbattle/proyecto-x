<!DOCTYPE html>
<html lang="en">

<%- include('partials/head') %>
<link rel="stylesheet" href="stylesheet/contact.css">

<body>
<div class="main">
    <%- include('partials/header') %>

    <div class="container">
        <section class="section-login-container">

            <form action="" method="POST">
              <p class="section-login-title">Contactanos</p>
              <div class="inputs">
                <label for="email">Email</label>
                <input type="text" name="email" id="" class="section-input">
              </div>
              <div class="inputs">
                <label for="Nombre">Tu nombre</label>
                <input type="text" name="nombre" id="" class="section-input">
              </div>
              <div class="remember">
                <input type="checkbox" name="rememberMe" id="" placeholder="Recordar usuario y contraseña" class="check">
                <label class="remember-me" for="">Recordar usuario y contraseña</label>
              </div>
              <div class="login">
                <button type="submit">Ingresar</button>
              </div>
              <div class="section-links">
                <a href="/forget-my-password">Olvidé mi contraseña</a>
              </div>
              <div class="section-links">
                <a href="/user/register">No estoy registrado</a>
              </div>
            </form>
            </div>
          </section>
        <div>

        </div>
    </div>

    <%- include('partials/footer') %>
</div>
</body>

</html>